<% if f.object.class.multiple? key %>
 <%#= f.input :yy, as: :extra, input_html: { :style => 'width: 100px'} %>/
 <%= f.input key, input_html: { :style=> 'width: 100px' }, required: f.object.required?(key) %>
<% else %>
  <%= f.input key, required: f.object.required?(key), input_html: { class: 'form-control' } %>
<% end %>

